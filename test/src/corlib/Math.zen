namespace System::Math;

public const double PI = 3.14159265358979323846;
public const double E  = 2.7182818284590452354;

public function int Add(let int a, let int b)
{
    return a + b;
}

public function int Sub(let int a, let int b)
{
    return a - b;
}

public function int Mul(let int a, let int b)
{
    return a * b;
}

public function int Div(let int a, let int b)
{
    return a / b;
}

public function int Mod(let int a, let int b)
{
    return a % b;
}
