namespace Program::Math;

public fun int Add(int a, int b)
{
	return a + b;
}

public fun int Sub(int a, int b)
{
    return a - b;
}

public fun int Mul(int a, int b)
{
    return a * b;
}

public fun int Div(int a, int b)
{
    return a / b;
}

public fun int Mod(int a, int b)
{
    return a % b;
}