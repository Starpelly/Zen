public fun void PrintMath()
{
	printf("Add: %i\n", Math::Add(2, 2));
	printf("Sub: %i\n", Math::Sub(2, 2));
	printf("Mul: %i\n", Math::Mul(2, 2));
	printf("Div: %i\n", Math::Div(2, 2));
	printf("Mod: %i\n", Math::Mod(2, 2));
}